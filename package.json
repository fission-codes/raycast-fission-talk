{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "fission-talk",
  "title": "Fission Talk",
  "description": "Interface for https://talk.fission.codes",
  "icon": "command-icon.png",
  "author": "icidasset",
  "license": "MIT",
  "commands": [
    {
      "name": "latest-topics",
      "title": "Latest topics",
      "description": "Lists the latest topics",
      "mode": "view"
    },
    {
      "name": "search-topics",
      "title": "Search topics",
      "description": "Searches topics by title",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.29.1",
    "cross-fetch": "^3.1.5"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
